{
  "env": {
    "node": true,
    "jquery": true
  },
  "globals": {},
  "rules": {
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "curly": 2,
    "eqeqeq": [2, 'allow-null'],
    "no-eq-null": 0,
    "no-unused-expressions": 0,
    "wrap-iife": [
      2,
      "any"
    ],
    "no-caller": 2,
    "quotes": [
      2,
      "double"
    ],
    "no-unused-vars": [
    	0,
    	{ "args": "none" }
	]
  }
}
